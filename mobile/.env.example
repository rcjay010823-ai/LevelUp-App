# Base URL for your backend API
# For local development, this might be http://localhost:3000
# For production, use your deployed domain
EXPO_PUBLIC_BASE_URL=https://your-domain.com

# The host domain (without https://)
EXPO_PUBLIC_HOST=your-domain.com

# Your project group ID from Create.xyz or your backend
EXPO_PUBLIC_PROJECT_GROUP_ID=your-project-id

# Optional: Proxy base URL if using a separate proxy server
EXPO_PUBLIC_PROXY_BASE_URL=https://your-proxy-domain.com
